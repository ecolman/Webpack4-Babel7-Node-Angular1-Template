{
  "name": "template",
  "version": "0.0.1",
  "main": "server/index.js",
  "engines": {
    "node": "^8.0.0",
    "npm": "^3.9.5"
  },
  "scripts": {
    "start:dev": "npm-run-all --parallel start:client start:server",
    "start:client": "webpack-dev-server --config webpack.client.config.js --progress",
    "start:server": "webpack --config webpack.server.config.js --progress",
    "start:server:nodemon": "nodemon .tmp/server.bundle.js -w .tmp/server.bundle.js --delay 1",
    "build:app": "npm-run-all --parallel build:client build:server --sequential zip",
    "build:client": "webpack --config webpack.client.config.js --progress --mode production",
    "build:server": "npm-run-all --sequential build:server:webpack build:server:babel",
    "build:server:webpack": "webpack --config webpack.server.config.js --progress --mode production",
    "build:server:babel": "babel server --source-maps --out-dir dist/server",
    "migrations": "babel-node server/sqldb/migrations/"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.20",
    "angular": "~1.7.0",
    "angular-animate": "~1.7.0",
    "angular-cookies": "~1.7.0",
    "angular-resource": "~1.7.0",
    "angular-socket-io": "~0.7.0",
    "angular-ui-bootstrap": "^2.5.6",
    "bootstrap": ">=3.4.1",
    "bootstrap-sass": ">=3.4.1",
    "composable-middleware": "^0.3.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-sequelize-session": "^0.4.0",
    "express-session": "^1.15.6",
    "font-awesome": ">=4.1.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "lusca": "^1.6.1",
    "method-override": "^2.3.10",
    "morgan": "~1.9.0",
    "multer": "^1.3.1",
    "passport": "~0.4.0",
    "passport-local": "^1.0.0",
    "sequelize": ">=5.3.0",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "socketio-jwt": "^4.5.0",
    "tedious": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "angular-mocks": "~1.6.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-import-glob": "^2.0.0",
    "bestzip": "^2.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "disable-output-webpack-plugin": "^1.0.1",
    "eslint": "^5.3.0",
    "eslint-loader": "^2.1.0",
    "file-loader": "^2.0.0",
    "generate-package-json-webpack-plugin": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "import-glob": "^1.5.0",
    "mini-css-extract-plugin": "^0.4.2",
    "ng-annotate-loader": "^0.6.1",
    "ng-annotate-patched": "^1.7.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
